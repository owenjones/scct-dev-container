{
    "name": "SCCT Development Environment",
    "dockerComposeFile": "docker-compose.yml",
    "service": "ubuntu",
    "workspaceFolder": "/home/scct",
    "customizations": {
        "vscode": {
            "extensions": [
                "ms-python.python"
            ]
        }
    },
    "remoteUser": "scct",
    "overrideCommand": true,
    "forwardPorts": [
        80,
        8080,
        8081
    ],
    "postStartCommand": "/init/post-start.sh",
    "shutdownAction": "stopCompose"
}